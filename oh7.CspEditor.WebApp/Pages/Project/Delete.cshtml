@page
@model oh7.CspEditor.WebApp.Pages.Project.DeleteModel

@{
    ViewData["Title"] = "Delete CSP Project";
}

<h3>@ViewData["Title"]</h3>
<div>
    <h4>Are you sure you want to delete this?</h4>

    <partial name="_Details" model="Model.CspProject" />
    
    <form method="post">
        <input type="hidden" asp-for="CspProject.Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> <a asp-page="./Index" class="btn btn-secondary">Cancel</a>
    </form>
</div>
